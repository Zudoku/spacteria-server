const pg = require('pg');
const pgconfig = {
  user: 'postgres',
  database: 'postgres',
  host: 'db',
  password: 'postgres',
  port: 5432,
  max: 10,
  idleTimeoutMills: 30000,
};

const pool = new pg.Pool(pgconfig);
pool.connect((err, client, done) => {
  console.log(err);
  console.log(client);
  console.log(done);
  client.query('SELECT * FROM gamecharacter',[], (err, result) => {
    console.log(result.rows, err);
  });
});
